{
  "$schema": "http://json-schema.org/schema",
  "$id": "CreatePackage",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use for the library? (WITHOUT @fluentui-contrib/)"
    },
    "owner": {
      "type": "string",
      "description": "Team owning the library. Will be written in CODEOWNERS",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "x-priority": "important",
      "x-prompt": {
        "message": "Which team will own this library",
        "type": "list",
        "items": [
          "@microsoft/cxe-red",
          "@microsoft/cxe-prg",
          "@microsoft/fluentui-react-build",
          "@microsoft/teams-prg"
        ]
      }
    }
  },
  "required": ["name", "owner"]
}
