{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", "storybook-static"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["stories/**/*.tsx", "stories/**/*.ts"],
      "rules": {
        "@nx/enforce-module-boundaries": "off"
      }
    }
  ]
}
